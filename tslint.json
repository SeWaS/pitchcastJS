{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "quotemark": [
            true,
            "single"
        ],
        "eofline": true,
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "semicolon": [true, "never"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "no-console": [false, "log", "error"]
    },
    "rulesDirectory": []
}